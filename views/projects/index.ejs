<% include ../partials/header %>

<div class="project-header">
    <header>
        <div class="container">
            <h1 class="display-1">Student Projects</h1>       
        </div>
    </header>
</div>
    
    
        


   
    <section class="projects">
        <% projects.forEach(function(project){ %>
            <div class="project">
                <h3><%= project.name %></h3> 
                <img class="project-img" src="<%= project.img%>" alt="">
                <div class="project__overlay">                
                    <p>
                        <%= project.desc.substring(0,100);%>...
                    </p>                        
                </div>
                <div class="project-btn">
                    <a href="/projects/<%= project._id%>"><button class="btn btn-outline">View →</button></a>                 
                </div>                
            </div>
        <% }); %>
    </section>


